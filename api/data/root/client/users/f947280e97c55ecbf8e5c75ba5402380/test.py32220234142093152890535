import PIL
from PIL import Image
import pytesseract

# Open the image file
image = Image.open("test.jpeg")

# Use pytesseract to extract the text from the image
text = pytesseract.image_to_string(image)

print(text)
# Save the text as a .c file
with open("handwritten_code.c", "w") as file:
    file.write(text)

print("Text from image has been saved as 'handwritten_code.c'")
